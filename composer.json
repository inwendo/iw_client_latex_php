{
    "name": "inwendo/iw_client_latex_php",
    "version": "1.0.2",
    "description": "API Component for the inwendo Latex To PDF Converter",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "authors": [{
        "name": "Enno Steppat",
        "homepage": "https://inwendo.de"
    }],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6"
    },
    "autoload": {
        "psr-4": { "Inwendo\\Latex\\Common\\": "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Inwendo\\Latex\\Common\\": "test/" }
    }
}
